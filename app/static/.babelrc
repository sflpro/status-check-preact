{
  "plugins": [
        [
            "transform-react-jsx",
            {
              "pragma": "h"
            }
        ],
        [
            "transform-class-properties"
        ],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "presets": [
        "es2015",
        "react", 
        "stage-0",
        [
          "env",
          {
              "targets": {
                  "ie": "9",
                  "browsers": [
                      "ie >= 8"
                  ]
              }
          }
      ]
  ]
}